.row-fluid {
    position: relative;
    left: 14%;
    top: 90px;
    width: 70%;
}

.form-horizontal {
    position: relative;
    left: 15%;
    line-height: 33px;
}

.form-horizontal label {
    font-family: -webkit-pictograph;
    font-size: 14px;
    position: relative;
}

.form-horizontal input{
    position: relative;
    width: 490px;
    height: 30px;
    border: solid 1px #E5E5E5;
    border-radius: 3px;
}

.form-horizontal select {
    position: relative;
    width: 490px;
    height: 30px;
    border: solid 1px #E5E5E5;
    border-radius: 3px;
}
.bnt-submit {
    position: relative;
    top: 38px;
    width: 83px;
    height: 37px;
}