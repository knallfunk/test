package com.wisedu.cloud.smp.dal.sp.dao.impl;

import com.wisedu.cloud.smp.common.exception.DaoException;
import com.wisedu.cloud.smp.dal.sp.dao.ProxyApplyDao;
import com.wisedu.cloud.smp.dal.sp.mapper.ProxyApplyExtMapper;
import com.wisedu.cloud.smp.dal.sp.mapper.ProxyApplyMapper;
import com.wisedu.cloud.smp.dal.sp.model.ProxyApply;
import com.wisedu.cloud.smp.dal.sp.model.ProxyApplyExample;
import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Repository;

import java.util.List;

/**
 * Created by huangxuecan on 2016/3/5.
 */
@Repository("proxyApplyDao")
public class ProxyApplyDaoImpl implements ProxyApplyDao {
    @Autowired
    private ProxyApplyMapper proxyApplyMapper;

    @Autowired
    private ProxyApplyExtMapper proxyApplyExtMapper;
    @Override
    public void insertProxyApply(ProxyApply proxyApply) throws DaoException {
        try {
            proxyApplyMapper.insertSelective(proxyApply);
        } catch (Exception e) {
            throw new DaoException("",e);
        }
    }

    @Override
    public void batchInsertProxyApply(List<ProxyApply> proxyApplies) throws DaoException {
        try {
            proxyApplyExtMapper.batchInsertProxyApply(proxyApplies);
        } catch (Exception e) {
            throw new DaoException("",e);
        }
    }

    @Override
    public List<ProxyApply> getProxyApplyByExample(ProxyApplyExample proxyApplyExample) throws DaoException {
        try {
            return proxyApplyMapper.selectByExample(proxyApplyExample);
        } catch (Exception e) {
            throw new DaoException("",e);
        }
    }
}
